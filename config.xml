<?xml version="1.0" encoding="UTF-8" ?>
<widget xmlns   = "http://www.w3.org/ns/widgets"    
    xmlns:gap   = "http://phonegap.com/ns/1.0"    
    id          = "com.phonegap.example"    
    versionCode = "10"    
    version     = "1.0.0" >
<!-- versionCode is optional and Android only -->  
<name>Sharpkid</name>  
<description>Trivia App</description>  
<author href="https://cis.temple.edu/~tug36870/" email="tug36870@temple.edu">Kazmuir, Sierra, Juwan, Magnus </author>  
<content src="index.html" />  
<plugin name="cordova-plugin-whitelist" spec="1.3.3" />  
<access origin="*" /> 
<access origin="*.opentdb.com" /> 
<allow-intent href="http://*/*" />  
<allow-intent href="https://*/*" />  
<allow-intent href="tel:*" />  
<allow-intent href="sms:*" />  
<allow-intent href="mailto:*" />  
<allow-intent href="geo:*" />  
<!-- Allow links to example.com -->  
<allow-navigation href="http://cis-linux2.temple.edu/~tug36870/2305/sharpkid/*" />  
<!-- Wildcards are allowed for the protocol, as a prefix     to 
the host, or as a suffix to the path -->  
<allow-navigation href="*://*.temple.edu/*" />  
<!-- A wildcard can be used to whitelist the entire network,     
over HTTP and HTTPS.     *NOT RECOMMENDED* -->  
<allow-navigation href="*" />  
<platform name="android">      
  <allow-intent href="market:*" />  
</platform>  
<platform name="ios">      
  <allow-intent href="itms:*" />      
  <allow-intent href="itms-apps:*" />  
</platform>
</widget>